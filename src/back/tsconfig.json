{
	"compilerOptions": {
		"module": "CommonJS",
		"moduleResolution": "node",
		"noImplicitAny": true,
		"noImplicitThis": true,
		"strictNullChecks": true,
		"target": "ES2019",
		"esModuleInterop": true,
		"lib": ["es6", "esnext"],
		"outDir": "../../dist/back",
		"keyofStringsOnly": true,
		"checkJs": false,
		"allowJs": true,
		"resolveJsonModule": true,
		"sourceMap": true,
		"skipLibCheck": true
	},
	"include": ["./**/*", "./*"]
}
